<?xml version='1.0' encoding='utf-8'?>
<widget id="com.asreader.cdvdemo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>AsReaderCDV</name>
	<description>
		A sample Apache Cordova application for AsReader.
	</description>
	<author email="bsetsu@asx.co.jp" href="http://asreader.com">
		AsReader Team
	</author>
	<content src="index.html" />
	<plugin name="cordova-plugin-whitelist" version="1" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
</widget>
